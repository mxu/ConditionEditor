<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../condition.xsl"?>
<questionnaire>
	<question_group name="Diagnosis-specific Questions">
		<question mandatory="true" prompt="What is the principal (chief) question or concern you have regarding your current condition?">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem for which you are seeking a second opinion:">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="When was your diagnosis made?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="What is the diagnosis?">
			<free_form_response height="3"/>
		</question>
		<question mandatory="true" prompt="When were you last examined by your physician for your current condition (one you are requesting a second opinion for)?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="What is your occupation?">
			<free_form_response height="3"/>
		</question>
		<question mandatory="true" prompt="Do you have any siblings (brothers/sisters)?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have any siblings, please complete the following information:">
						<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Sibling (brother/sister)" width="20"/>
								<column_definition label="Age" width="15"/>
								<column_definition label="Current state of health, if known" width="35"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have a family history of any of the following?  (choose all that apply)">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have a family history of any of the following, please indicate below:">
						<table_response initial_rows_to_display="2" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Condition" width="30"/>
								<column_definition label="Relationship" width="20"/>
							</column_definitions>
							<table_data>
								<cell>Aneurysm</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Aortic dissection</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Bleeding disorder</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Blood clots</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Coronary artery disease</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Cancer</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>CVA (stroke)</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Diabetes</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Hypertension</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>COPD / emphysema</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Kidney disease</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Other vascular/circulatory problems not listed above</cell>
								<cell/>
							</table_data>
						</table_response>
					</question>
					<question mandatory="false" prompt="If other, please list below:">
						<free_form_response height="3"/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="What is your ancestral origin i.e.: Irish, Italian, German, etc.?">
			<free_form_response height="3"/>
		</question>
		<question mandatory="true" prompt="Have you received a blood or blood product (example: platelets) transfusion?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have received a blood or blood product (example: platelets) transfusion, please provide additional information below:">
						<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Type of transfusion" width="25"/>
								<column_definition label="Date (month, year)" width="20"/>
								<column_definition label="Adverse reaction(s), side effects (if any)" width="30"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever had a myocardial Infarction (heart attack)?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have ever had a myocardial Infarction, please indicate date:">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever been diagnosed with a deep venous thrombosis (DVT) (blood clot in leg)?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have ever been diagnosed with a deep venous thrombosis, please indicate date:">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever been diagnosed with a pulmonary embolism (PE) (blood clot in lung)?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have ever been diagnosed with a pulmonary embolism, please indicate date:">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever been diagnosed with a bleeding disorder?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have ever been diagnosed with a bleeding disorder, please provide additional information below:">
						<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Type of bleeding disorder" width="25"/>
								<column_definition label="Date of diagnosis (Month, Year)" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you bruise easily?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have a history of any of the following?  (choose all that apply)">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have a history of any of the following, please indicate below:">
