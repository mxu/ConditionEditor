								<cell/>
							</table_data>
							<table_data>
								<cell>Gout</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Hemophilia A</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Hemophilia B</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Hypercoagulable state</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Immune Thrombocytopenic Purpura (ITP)</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Intracardiac shunt</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Kidney tumors or cysts</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Marfan's Syndrome</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Liver hepatoma</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Pneumonia</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Shortness of breath</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Sleep apnea</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Thrombotic Thrombocytopenia Purpura (TTP)</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>TIA (transient ischemia attack)</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Uterine leiomyoma (female only)</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Von Wellebrand's Disease</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>None of the above</cell>
								<cell/>
							</table_data>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever undergone dental extractions?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you ever undergone dental extractions, please provide additional information below:">
						<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Date (Month, Year)" width="25"/>
								<column_definition label="Complications, if any" width="35"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Please check below any of the following procedures or surgeries you have had: (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Lower extremity (leg) bypass surgery"/>
				<selection selected="false" text="Carotid (neck) artery surgery"/>
				<selection selected="false" text="Abdominal aneurysm repair"/>
				<selection selected="false" text="Thoracic aneurysm repair"/>
				<selection selected="false" text="Amputation"/>
				<selection selected="false" text="Percutaneous intervention (balloon angioplasty)"/>
				<selection selected="false" text="Pacemaker insertion"/>
				<selection selected="false" text="Renal (kidney) artery intervention"/>
				<selection selected="false" text="Aortic surgical procedures"/>
				<selection selected="false" text="None of the above"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="What is your gender?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Male"/>
				<selection selected="false" text="Female (answer questions below)">
					<question mandatory="true" prompt="Have you ever been pregnant?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If you have ever been pregnant, how many times have you been pregnant?">
									<free_form_response height="3"/>
								</question>
								<question mandatory="true" prompt="How many live births have you had?">
									<free_form_response height="3"/>
								</question>
								<question mandatory="true" prompt="How many elective abortions have you had?">
									<free_form_response height="3"/>
								</question>
								<question mandatory="true" prompt="How many spontaneous abortions (miscarriages) have you had?">
									<free_form_response height="3"/>
								</question>
								<question mandatory="true" prompt="If you have experienced spontaneous abortions (miscarriages), please provide additional information below:">
									<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
