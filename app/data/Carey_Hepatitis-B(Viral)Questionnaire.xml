<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../condition.xsl"?>
<questionnaire>
	<question_group name="Diagnosis-specific Questions">
		<question mandatory="true" prompt="What is the principal (chief) question or concern you have regarding your current condition?">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem (one you are requesting a second opinion for):">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="What are the primary hopes and expectations that you have regarding this consultation?">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="As related to your current problem (condition for which you are seeking a second opinion), how was your diagnosis made? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="X-ray"/>
				<selection selected="false" text="Cholangiogram"/>
				<selection selected="false" text="Angiogram"/>
				<selection selected="false" text="Upper Endoscopy"/>
				<selection selected="false" text="CT Scan"/>
				<selection selected="false" text="MRI Scan"/>
				<selection selected="false" text="Ultrasound"/>
				<selection selected="false" text="Blood Work"/>
				<selection selected="false" text="Surgery"/>
				<selection selected="false" text="Liver Biopsy"/>
				<selection selected="false" text="Liver Scan"/>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If other, please list:">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="When was your diagnosis made?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="When were you last examined by your physician for this condition?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Does your mother have a history of hepatitis B?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you know how you contracted Hepatitis B?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>							
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
				<question mandatory="true" prompt="If you do know how you contracted Hepatitis B, please give details below:">
						<free_form_response/>
						</question>
				</selection>				
			</item_response>
		</question>
		<question mandatory="true" prompt="For how long have you had this problem?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Less than a month"/>
				<selection selected="false" text="1-3 months"/>
				<selection selected="false" text="4-6 months"/>
				<selection selected="false" text="7-12 months"/>
				<selection selected="false" text="Greater than 12 months"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Are your symptoms?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="Getting worse"/>
				<selection selected="false" text="Getting better"/>
				<selection selected="false" text="Staying the same"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you feel as if your general health has deteriorated as a result of this condition?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have a history of seizures?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have a history of seizures, when was your last seizure?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have a history of diabetes?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have a history of diabetes, do you have eye involvement / visual complications as a result of the diabetes?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you been told that you have renal (kidney) involvement as a result of your current condition?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have been told that you have renal (kidney) involvement as a result of your current condition, please give details below:">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Are you of Asian decent?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever been vaccinated against hepatitis A? (this  usually involves 2 injections)">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have ever been vaccinated against hepatitis A , when did you receive the vaccination?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever been vaccinated against hepatitis B ? (this  usually involves 3 injections)">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have ever been vaccinated against hepatitis B, when did you receive the vaccination?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have your family members and sexual partners been vaccinated against Hepatitis?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever participated in the following high risk activities? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Homosexuality"/>
				<selection selected="false" text="Illicit IV drug use / abuse"/>
				<selection selected="false" text="Illicit Intra-nasal drug use / abuse"/>
				<selection selected="false" text="Multiple sexual partners"/>
				<selection selected="false" text="None of the above"/>
