						<free_form_response height="3"/>
					</question>
					
					<question mandatory="true" prompt="If yes, how many times per day and days per week does the child receive IV fluids?">
						<free_form_response height="3"/>
					</question>
					
					<question mandatory="true" prompt="Who is managing the child's IV fluids?">
						<table_response add_row_increment="1" initial_rows_to_display="3" row_height="2">
							<column_definitions>
								<column_definition label="Physician Name" width="20"/>
								<column_definition label="Address" width="20"/>
