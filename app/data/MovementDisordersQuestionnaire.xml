							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If you have times when your medication is working,  on average how long does each medication dose last?  (# of hours)">
									<free_form_response height="1"/>
								</question>
								<question mandatory="true" prompt="What percentage of your waking hours do you feel it is working?">
