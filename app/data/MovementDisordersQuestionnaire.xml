<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../condition.xsl"?>
<questionnaire>
	<question_group name="Diagnosis-specific Questions">
     
		<question mandatory="true" prompt="What is the principal (chief) question or concern you have regarding your current condition?">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem for which you are requesting a second opinion:">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="What is your diagnosis? (choose one)">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Parkinson's Disease"/>
				<selection selected="false" text="Essential or other tremor"/>
				<selection selected="false" text="Dystonia"/>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If other please explain: ">
						<free_form_response height="3"/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Are you? (choose one)">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Right handed"/>
				<selection selected="false" text="Left handed"/>
				<selection selected="false" text="Ambidextrous"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="When did your symptoms begin?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Describe your initial symptoms below (be as specific as possible in your description):">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="When was your diagnosis made?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Did initial medical therapy (medications) improve your symptoms?  (Please describe below.)">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="When did treatment or symptoms become difficult to treat with medication?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Describe your current problems and symptoms related to your movement disorder:">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="Please list the three top symptoms or problems you would like deep brain stimulation to help:">
			<free_form_response height="7"/>
		</question>
		<question mandatory="true" prompt="When were you last examined by your physician for your current condition (the one you are requesting a second opinion for)?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Please complete the table below by listing all of the diagnostic tests you have had.  Also provide us with the date of the test, where it was done, and the results (normal vs. abnormal).">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Brain or spine MRI">
					<question mandatory="true" prompt="">
						<table_response initial_rows_to_display="1" add_row_increment="1" row_height="1">
							<column_definitions>
								<column_definition label="Date (Month, Year)" width="20"/>
								<column_definition label="Hospital, clinic, diagnostic center" width="30"/>
								<column_definition label="City, State" width="20"/>
								<column_definition label="Result of test"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
				<selection selected="false" text="Brain or spine CT scan">
					<question mandatory="true" prompt="">
						<table_response initial_rows_to_display="1" add_row_increment="1" row_height="1">
							<column_definitions>
								<column_definition label="Date (Month, Year)" width="20"/>
								<column_definition label="Hospital, clinic, diagnostic center" width="30"/>
								<column_definition label="City, State" width="20"/>
								<column_definition label="Result of test" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
				<selection selected="false" text="Brain EEG (Electroencephalogram)">
					<question mandatory="true" prompt="">
						<table_response initial_rows_to_display="1" add_row_increment="1" row_height="1">
							<column_definitions>
								<column_definition label="Date (Month, Year)" width="20"/>
								<column_definition label="Hospital, clinic, diagnostic center" width="30"/>
								<column_definition label="City, State" width="20"/>
								<column_definition label="Result of test" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
				<selection selected="false" text="EMG and / or NCS">
					<question mandatory="true" prompt="">
						<table_response initial_rows_to_display="1" add_row_increment="1" row_height="1">
							<column_definitions>
								<column_definition label="Date (Month, Year)" width="20"/>
								<column_definition label="Hospital, clinic, diagnostic center" width="30"/>
								<column_definition label="City, State" width="20"/>
								<column_definition label="Result of test" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
				<selection selected="false" text="Spinal tap">
					<question mandatory="true" prompt="">
						<table_response initial_rows_to_display="1" add_row_increment="1" row_height="1">
							<column_definitions>
								<column_definition label="Date (Month, Year)" width="20"/>
								<column_definition label="Hospital, clinic, diagnostic center" width="30"/>
								<column_definition label="City, State" width="20"/>
								<column_definition label="Result of test" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
				<selection selected="false" text="Lab tests specific to your problem">
					<question mandatory="true" prompt="">
						<table_response initial_rows_to_display="1" add_row_increment="1" row_height="1">
							<column_definitions>
								<column_definition label="Date (Month, Year)" width="20"/>
								<column_definition label="Hospital, clinic, diagnostic center" width="30"/>
								<column_definition label="City, State" width="20"/>
								<column_definition label="Result of test" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="">
						<table_response initial_rows_to_display="1" add_row_increment="1" row_height="1">
							<column_definitions>
								<column_definition label="Date (Month, Year)" width="20"/>
								<column_definition label="Hospital, clinic, diagnostic center" width="30"/>
								<column_definition label="City, State" width="20"/>
								<column_definition label="Result of test" width="20"/>
							</column_definitions>
						</table_response>
					</question>
					<question mandatory="true" prompt="If other, please list:">
						<free_form_response height="3"/>
					</question>
				</selection>
                <selection selected="false" text="None of the above"/>
			</item_response>            
		</question>
		<question mandatory="true" prompt="In the table below, please list all medications taken, the dosage of the medication and the time taken.">
			<table_response initial_rows_to_display="3" add_row_increment="2">
				<column_definitions>
					<column_definition label="Medication" width="20"/>
					<column_definition label="Dosage" width="10"/>
					<column_definition label="Date prescribed" width="10"/>
					<column_definition label="Date stopped" width="10"/>
				</column_definitions>
			</table_response>
		</question>
