						<free_form_response height="2"/>
					</question>
					<question mandatory="true" prompt="When was the last time? (Date: month/year)">
						<free_form_response height="2"/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you ever experience bouncing of the environment when taking a step (while walking)?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever had ear, brain or neck surgery?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you ever had ear, brain or neck surgery, please provide the following in table format:">
						<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Month/Year" width="15"/>
								<column_definition label="Type of surgery" width="30"/>
								<column_definition label="Physician" width="25"/>
								<column_definition label="Hospital/Clinic" width="25"/>
								<column_definition label="City, State" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you experience dizziness?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you experience dizziness, when did your dizziness originally start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
					<question mandatory="true" prompt="Please indicate which of the following best describe your dizziness sensation: (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="I feel like I'm whirling or moving"/>
							<selection selected="false" text="I feel like things around me are whirling or moving"/>
							<selection selected="false" text="I feel as though I'm floating"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If other, please describe:">
									<free_form_response height="3"/>
								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is your dizziness accompanied by headache?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is your dizziness accompanied by hearing change?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes, on Left side"/>
							<selection selected="false" text="Yes, on Right side"/>
							<selection selected="false" text="Yes, both sides"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is your dizziness accompanied by disorientation (inability to sense direction or location)?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is your dizziness accompanied by noises in your ear?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes, on Left side"/>
							<selection selected="false" text="Yes, on Right side"/>
							<selection selected="false" text="Yes, both sides"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is your dizziness accompanied by loss of balance?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is your dizziness accompanied by neck pain?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes, on Left side"/>
							<selection selected="false" text="Yes, on Right side"/>
							<selection selected="false" text="Yes, both sides"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is your dizziness accompanied by nausea?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="How often are you dizzy?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="All the time"/>
							<selection selected="false" text="Whenever you change position"/>
							<selection selected="false" text="In episodes">
								<question mandatory="true" prompt="If your dizziness occurs in episodes, how long does an episode last? (# of minutes, hours, etc.)">
									<free_form_response height="3"/>
								</question>
								<question mandatory="true" prompt="When were your last two episodes of dizziness?">
									<table_response initial_rows_to_display="2" add_row_increment="0" row_height="2">
										<column_definitions>
											<column_definition label="Day" width="15"/>
											<column_definition label="Month" width="30"/>
											<column_definition label="Year" width="25"/>
										</column_definitions>
									</table_response>
								</question>
								<question mandatory="true" prompt="Do you have any warning that an episode of dizziness will occur?">
									<item_response drop_down_style="false" single_response="true">
										<selection selected="false" text="Not applicable"/>
										<selection selected="false" text="No"/>
										<selection selected="false" text="Yes">
											<question mandatory="true" prompt="If you have any warning that an episode of dizziness will occur, please explain:">
												<free_form_response height="3"/>
											</question>
										</selection>
									</item_response>
								</question>
								<question mandatory="true" prompt="Which of the following can make your dizziness worse or trigger an episode? (choose all that apply)">
									<item_response drop_down_style="false" single_response="false">
										<selection selected="false" text="When you're fatigued (tired)">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Menstrual period">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Rolling over in bed to right side">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Rolling over in bed to left side">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Riding in the car">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Driving a car">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Bending or stooping over">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="When you first wake up">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="With a weather change">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Moving quickly">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Turning your head">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Looking up">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="At the end of the day">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="With stress">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="With worry">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Watching things move past you">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="Coughing or straining">
											<question mandatory="true" prompt="">
												<item_response drop_down_style="false" single_response="true">
													<selection selected="false" text="Triggers"/>
													<selection selected="false" text="Makes worse"/>
												</item_response>
											</question>
										</selection>
										<selection selected="false" text="None of the above"/>
									</item_response>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have problems with your balance?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have problems with your balance, when did you begin to lose your balance?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
					<question mandatory="true" prompt="How has your balance been affected?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Suddenly"/>
							<selection selected="false" text="Gradually"/>
							<selection selected="false" text="In episodes">
								<question mandatory="true" prompt="If it occurs in episodes, for how long does a typical episode last?">
									<free_form_response height="3"/>
								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="Do you ever fall as a result of losing your balance?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If you ever fall as a result of losing your balance, how often do you fall?">
									<free_form_response height="3"/>
								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="Does dizziness accompany your loss of balance?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Always"/>
							<selection selected="false" text="Sometimes"/>
							<selection selected="false" text="Never"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="How do you veer (feel pushed) when you walk?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="To the right"/>
							<selection selected="false" text="To the left"/>
							<selection selected="false" text="Either side"/>
							<selection selected="false" text="None of the above"/>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
