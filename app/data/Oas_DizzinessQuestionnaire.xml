				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have hyperlipidemia (high fat levels in the blood)?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have hyperlipidemia, what was your last level (if known)?">
						<free_form_response height="2"/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have hypercholesterolemia (high cholesterol levels in the blood)?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have hypercholesterolemia, what was your last level (if known)?">
						<free_form_response height="2"/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever been exposed to loud noises for prolonged time periods?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have ever been exposed to loud noises for prolonged time periods, what is the duration of the noise exposure? (months/years)">
						<free_form_response height="2"/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you ever participate in underwater diving?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
