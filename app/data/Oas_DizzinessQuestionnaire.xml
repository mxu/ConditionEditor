							</table_data>
							<table_data>
								<cell>Subelavian Steal Syndrome</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Orthostatic Hypotension</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Anemia</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Hypoglycemia</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Anxiety Disorder</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Cerebral Ischemia</cell>
								<cell/>
							</table_data>
							<table_data>
								<cell>Wallenberg Syndrome</cell>
								<cell/>
							</table_data>
