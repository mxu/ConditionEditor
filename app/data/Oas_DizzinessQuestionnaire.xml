							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="How long do the headaches usually last? (minutes, hours, etc.)">
						<free_form_response height="3"/>
					</question>
					<question mandatory="true" prompt="Are the headaches ever accompanied by dizziness?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Usually"/>
							<selection selected="false" text="Sometimes"/>
							<selection selected="false" text="Never"/>
