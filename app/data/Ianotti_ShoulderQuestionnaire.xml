<?xml-stylesheet type="text/xsl" href="../condition.xsl"?>
<questionnaire>
	<question_group name="Diagnosis-specific Questions">
		<question mandatory="true" prompt="What is the principal (chief) question or concern you have regarding your current condition?">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem for which you are requesting a second opinion:">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="What is your occupation?">
			<free_form_response/>
		</question>
		<question mandatory="true" prompt="What is your employment status?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Employed full time"/>
				<selection selected="false" text="Employed part time"/>
				<selection selected="false" text="Unemployed"/>
				<selection selected="false" text="Sick leave"/>
				<selection selected="false" text="Homemaker"/>
				<selection selected="false" text="Student"/>
				<selection selected="false" text="Retired"/>
				<selection selected="false" text="Temporarily disabled"/>
				<selection selected="false" text="Permanently disabled"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Which shoulder is affected?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Right"/>
				<selection selected="false" text="Left"/>
				<selection selected="false" text="Both"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="When did you first start to experience shoulder problems?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Was your current shoulder problem caused by an injury?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If your current shoulder problem was caused by an injury, what type of injury?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Job"/>
							<selection selected="false" text="Fall"/>
							<selection selected="false" text="MVA (motor vehicle accident)"/>
							<selection selected="false" text="Lifting"/>
							<selection selected="false" text="Sports"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If other, please list:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="false" prompt="If known, what is your shoulder disorder diagnosis?">
			<free_form_response height="3"/>
		</question>
		<question mandatory="true" prompt="How was your diagnosis made?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Physical exam"/>
				<selection selected="false" text="X-rays"/>
				<selection selected="false" text="CT"/>
				<selection selected="false" text="MRI"/>
				<selection selected="false" text="Unknown"/>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If other, please list:">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="When was your diagnosis made?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Please indicate on the scale below your satisfaction with the current level of function of your affected shoulder: (1= not satisfied, 10= very satisfied)">
			<item_response drop_down_style="true" single_response="true">
							<selection selected="false" text="10 - Very satisfied"/>
							<selection selected="false" text="9"/>
							<selection selected="false" text="8"/>
							<selection selected="false" text="7"/>
							<selection selected="false" text="6"/>
							<selection selected="false" text="5 - Satisfied"/>
							<selection selected="false" text="4"/>
							<selection selected="false" text="3"/>
							<selection selected="false" text="2"/>
							<selection selected="false" text="1 - Not at all satisfied"/>
						</item_response>

		</question>
		<question mandatory="true" prompt="When were you last examined?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Are you experiencing pain?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you are experiencing pain, what is the location of the pain?">
						<free_form_response height="2"/>
					</question>
					<question mandatory="true" prompt="If you are experiencing pain, does the pain prevent you from falling asleep or wake you during the night?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Please rate your pain below: (1= Minimal pain, 10= Extreme pain)">
