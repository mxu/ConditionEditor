<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../condition.xsl"?>
<!-- edited with XML Spy v3.5 NT (http://www.xmlspy.com) by Tony Hardee (Perot Systems) -->
<questionnaire>
	<question_group name="Diagnosis-specific Questions">
		<question mandatory="true" prompt="What is the principal (chief) question or concern you have regarding infant's current condition?">
			<free_form_response height="5"/>
		</question>
		
		<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem for which you are requesting a second opinion.">
			<free_form_response height="5"/>
		</question>
		
		<question mandatory="true" prompt="What diagnosis was given for the infant's condition? (please provide exact diagnosis if known)">
			<free_form_response height="5"/>
		</question>
		
		<question mandatory="true" prompt="When was the diagnosis made?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		
		<question mandatory="true" prompt="What are the infant's current active gastrointestinal symptoms related to this condition? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Nausea"/>
				<selection selected="false" text="Bloating"/>
				<selection selected="false" text="Vomiting"/>
				<selection selected="false" text="Constipation"/>
				<selection selected="false" text="Diarrhea"/>
				<selection selected="false" text="Excessive Gas"/>
				<selection selected="false" text="Loss of appetite"/>
				<selection selected="false" text="Acid reflux"/>
				<selection selected="false" text="Other/Comment">
					<question mandatory="true" prompt="If Other/Comment, please explain:">
						<free_form_response height="3"/>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="What is the infant's current activity level related to this condition? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Can Sit"/>
				<selection selected="false" text="Can Stand"/>
				<selection selected="false" text="Bedridden"/>
				<selection selected="false" text="Ambulatory"/>
				<selection selected="false" text="Able to perform activities of daily living"/>
				<selection selected="false" text="Other/Comment">
					<question mandatory="true" prompt="If Other/Comment, please explain:">
						<free_form_response height="3"/>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Has the infant ever been diagnosed with a Vitamin or Mineral deficiency?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not Sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, list the type of deficiency">
						<free_form_response height="3"/>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Is the infant currently taking any vitamin or mineral supplements for this condition?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, please complete the table below">
						<table_response add_row_increment="1" initial_rows_to_display="3" row_height="2">
							<column_definitions>
								<column_definition label="Name of  Supplement" width="15"/>
								<column_definition label="Dosage" width="25"/>
								<column_definition label="How often, When taken" width="25"/>
								<column_definition label="Effectiveness of medication" width="25"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Is the infant currently taking any medications for this condition?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, please complete the table below">
						<table_response add_row_increment="1" initial_rows_to_display="3" row_height="2">
							<column_definitions>
								<column_definition label="Name of  Drug" width="15"/>
								<column_definition label="Dosage" width="25"/>
								<column_definition label="How often, When taken" width="25"/>
								<column_definition label="Effectiveness of medication" width="25"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
				
		<question mandatory="true" prompt="Please list all treatments the infant has received for this condition (condition you are seeking a second opinion for)">
			<table_response add_row_increment="1" initial_rows_to_display="4" row_height="2">
				<column_definitions>
					<column_definition label="Treatment/Medication" width="25"/>
					<column_definition label="Date(s) of treatment" width="20"/>
					<column_definition label="Treating Physician" width="25"/>
					<column_definition label="City" width="25"/>
					<column_definition label="State" width="25"/>
					<column_definition label="Effectiveness of treatment" width="25"/>
				</column_definitions>
			</table_response>
		</question>
		
		<question mandatory="true" prompt="Did the infant's mother experience any of the following problems with her pregnancy? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Fever"/>
				<selection selected="false" text="Infection"/>
				<selection selected="false" text="Antibiotic administration"/>
				<selection selected="false" text="Other medication administration ">
					<question mandatory="true" prompt="If other medication, please give detail:">
						<free_form_response height="2"/>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Did the infant go home with mother within 2 - 7 days following birth? (if known)">
			<item_response single_response="true" drop_down_style="false">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="Yes"/>
				<selection selected="false" text="No">
					<question mandatory="true" prompt="If no, please explain the details of the continued hospital stay:">
						<free_form_response height="5"/>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Were there any problems with the delivery of this infant?">
			<item_response single_response="true" drop_down_style="false">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, please explain in detail :">
						<free_form_response height="5"/>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Was the infant born at full term?">
			<item_response single_response="true" drop_down_style="false">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="Yes"/>
				<selection selected="false" text="No">
					<question mandatory="true" prompt="If no, at how many weeks gestation was infant born?">
						<free_form_response height="5"/>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="What was the infant's weight at birth?">
			<table_response initial_rows_to_display="1" add_row_increment="0" row_height="2">
				<column_definitions>
					<column_definition label="# of pounds / kilograms" width="15"/>
					<column_definition label="# of ounces / grams" width="15"/>
				</column_definitions>
			</table_response>
		</question>
		
		<question mandatory="true" prompt="What was the infant's length at birth? (# of inches)">
			<free_form_response height="1"/>
		</question>
		
		<question mandatory="true" prompt="What was the infant's head circumference at birth? (# of inches)">
			<free_form_response height="1"/>
		</question>
		
		<question mandatory="true" prompt="What is the infant's current weight (within the last month)?">
			<table_response initial_rows_to_display="1" add_row_increment="0">
				<column_definitions>
					<column_definition label="Date" width="15"/>
					<column_definition label="# of pounds" width="15"/>
					<column_definition label="# of ounces" width="15"/>
				</column_definitions>
			</table_response>
		</question>
		
		<question mandatory="true" prompt="What is the infant's current length?">
			<table_response initial_rows_to_display="1" add_row_increment="0">
				<column_definitions>
					<column_definition label="Date" width="15"/>
					<column_definition label="# of inches" width="15"/>
				</column_definitions>
			</table_response>
		</question>

		<question mandatory="true" prompt="What is the infant's current head circumference?">
			<table_response initial_rows_to_display="1" add_row_increment="0">
				<column_definitions>
					<column_definition label="Date" width="15"/>
					<column_definition label="# of inches" width="15"/>
				</column_definitions>
			</table_response>
		</question>
				
		<question mandatory="true" prompt="Did the infant have an intensive care (PICU or NICU) stay following his/her birth?">
			<item_response single_response="true" drop_down_style="false">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, how long was the infant hospitalized?">
						<free_form_response height="5"/>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="What is the current method of feeding that the infant is receiving? (choose all that apply)">
			<item_response single_response="false" drop_down_style="false">
				<selection selected="false" text="Oral"/>
				<selection selected="false" text="Tube Feeding"/>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If Other, Please List?">
						<free_form_response height="5"/>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="What type of feedings does the infant currently receive?">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Breast-fed">
					<question mandatory="true" prompt="If yes, how many times per day?">
						<free_form_response height="2"/>
					</question>
				</selection>
				
				<selection selected="false" text="Expressed breast milk by bottle">
					<question mandatory="true" prompt="If yes, how many ounces of expressed milk by bottle does the infant receive in 24 hours?">
						<free_form_response height="2"/>
					</question>
				</selection>

				<selection selected="false" text="Formula">
					<question mandatory="true" prompt="If the infant receives formula, what is the name of the formula?">
						<free_form_response height="2"/>
					</question>
					
					<question mandatory="true" prompt="What type of formula is used?">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Ready to feed"/>
							<selection selected="false" text="Liquid concentrate">
								<question mandatory="true" prompt="If concentrate is used, how is it mixed?">
									<table_response initial_rows_to_display="1" add_row_increment="0">
										<column_definitions>
											<column_definition label="Amount of concentrate (ounces)" width="15"/>
											<column_definition label="Amount of water (ounces)" width="15"/>
										</column_definitions>
									</table_response>
								</question>
							</selection>
							<selection selected="false" text="Powder">
								<question mandatory="true" prompt="If powder is used, how is it mixed?">
									<table_response initial_rows_to_display="1" add_row_increment="0">
										<column_definitions>
											<column_definition label="Amount of powder  (# of scoops)" width="15"/>
											<column_definition label="Amount of water (ounces)" width="15"/>
										</column_definitions>
									</table_response>
								</question>
							</selection>
						</item_response>
					</question>
					
					<question mandatory="true" prompt="Is anything added to the infant's formula? (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Fiber"/>
							<selection selected="false" text="Vitamins"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If  other, Please list">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					
					<question mandatory="true" prompt="How many ounces of formula does the infant drink per feeding?">
						<free_form_response height="2"/>
					</question>

					<question mandatory="true" prompt="How many ounces of formula does the infant drink in 24 hours?">
						<free_form_response height="2"/>
					</question>
				
					<question mandatory="true" prompt="Does the infant drink anything other than formula?">
						<item_response single_response="true" drop_down_style="false">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If yes, please complete the following table">
									<table_response initial_rows_to_display="3" add_row_increment="1">
										<column_definitions>
											<column_definition label="Type of Fluid" width="25"/>
											<column_definition label="Ounces in 24 hour period" width="25"/>
										</column_definitions>
										<table_data>
											<cell>Juice</cell>
											<cell></cell>
										</table_data>
										<table_data>
											<cell>Water</cell>
											<cell></cell>
										</table_data>
										<table_data>
											<cell>Other (Please list here)</cell>
											<cell></cell>
										</table_data>
									</table_response>
								</question>
							</selection>
						</item_response>
					</question>
		
				</selection>
			</item_response>
		</question>
					
		<question mandatory="true" prompt="At what month did the infant start solid foods?">
			<free_form_response height="2"/>
		</question>
		
		<question mandatory="true" prompt="Is the infant allergic to any foods?">
			<item_response single_response="true" drop_down_style="false">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, please complete the following table">
						<table_response initial_rows_to_display="3" add_row_increment="1">
							<column_definitions>
								<column_definition label="Food" width="25"/>
								<column_definition label="Type of reaction" width="25"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Is the infant currently able to tolerate oral feedings?">
			<item_response single_response="true" drop_down_style="false">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
				
					<question mandatory="true" prompt="Please check all the foods that the infant is able to eat / consume">
						<item_response single_response="false" drop_down_style="false">
							<selection selected="false" text="Liquids"/>
							<selection selected="false" text="Formula"/>
							<selection selected="false" text="Solid / Soft Foods">
							
								<question mandatory="true" prompt="Please describe in detail th solid food intake of the infant in a 24 hour period (Please include each meal in detail pertaining to all solids and liquids">
									<table_response initial_rows_to_display="1" add_row_increment="0" row_height="2">
										<column_definitions>
											<column_definition label="Description&lt;BR&gt;Example: Breakfast: 1/4 cup rice cereal, &lt;BR&gt;2.5 ounces applesauce, 3 ounces juice" width="40"/>
										</column_definitions>
									</table_response>
								</question>
							</selection>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If Other, Please list">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					
				</selection>
			</item_response>
		</question>
		
		
		<question mandatory="true" prompt="Does the infant experience any adverse symptoms after eating?">
			<item_response single_response="true" drop_down_style="false">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
				
					<question mandatory="true" prompt="Which of the following adverse symptoms does the chld experience after eating? (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="None"/>
							<selection selected="false" text="Nausea"/>
							<selection selected="false" text="Bloating"/>
							<selection selected="false" text="Vomiting"/>
							<selection selected="false" text="Constipation"/>
							<selection selected="false" text="Diarrhea"/>
							<selection selected="false" text="Excessive Gas"/>
							<selection selected="false" text="Loss of appetite"/>
							<selection selected="false" text="Acid reflux"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If other, please explain">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					
					<question mandatory="true" prompt="How soon after eating does the infant experience these symptoms?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Meals do not cause symptoms"/>
							<selection selected="false" text="Immediately"/>
							<selection selected="false" text="30 to 60 minutes"/>
							<selection selected="false" text="1 to 2 hours"/>
							<selection selected="false" text="Greater than 2 hours"/>
						</item_response>
					</question>
					
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Does the infant have an oral aversion (distaste) to any of the following?">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Liquids"/>
				<selection selected="false" text="Solids"/>
				<selection selected="false" text="Textures"/>
				<selection selected="false" text="Other/Comment">
					<question mandatory="true" prompt="If other, please explain">
						<free_form_response height="2"/>
					</question>
				</selection>
			</item_response>
		</question>
	
		<question mandatory="true" prompt="Does the infant have trouble with aspiration or choking?">
			<item_response single_response="true" drop_down_style="false">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
				
					<question mandatory="true" prompt="If yes, which of the following cause the infant a problem?">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Liquids"/>
							<selection selected="false" text="Solids"/>
							<selection selected="false" text="Textures"/>
							<selection selected="false" text="Other/Comment">
								<question mandatory="true" prompt="If other, please explain">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					
					<question mandatory="true" prompt="What recommendations or treatments have been tried?">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Thicken feeds"/>
							<selection selected="false" text="Oral feeding therapy"/>
							<selection selected="false" text="Nothing by mouth"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If Other, please explain">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Is the infant on any special type of diet?">
			<item_response single_response="true" drop_down_style="false">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, what type of diet is the infant on? (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Low sugar diet"/>
							<selection selected="false" text="High sugar diet"/>
							<selection selected="false" text="Low fat diet"/>
							<selection selected="false" text="High fat diet"/>
							<selection selected="false" text="Low salt diet"/>
							<selection selected="false" text="High salt diet"/>
							<selection selected="false" text="Low fiber diet"/>
							<selection selected="false" text="High fiber diet"/>
							<selection selected="false" text="Low protein diet"/>
							<selection selected="false" text="High protein diet"/>
							<selection selected="false" text="Fluid restriction"/>
							<selection selected="false" text="Gluten-free diet"/>
							<selection selected="false" text="Milk free diet"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If other, please explain">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Is the infant dependent on Total parenteral nutrition (TPN)?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not Sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
				
					<question mandatory="true" prompt="If yes, how long has the infant been receiving TPN?">
						<free_form_response height="3"/>
					</question>
					
					<question mandatory="true" prompt="If yes, how many days per week does the infant receive TPN fluids?">
						<free_form_response height="3"/>
					</question>
					
					<question mandatory="true" prompt="Is the infant receiving Omegavan (Specialty intravenous lipid (fat) solution)?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not Sure"/>
							<selection selected="false" text="No">
								<question mandatory="true" prompt="If no, is the infant receiving low fat TPN?">
									<item_response drop_down_style="false" single_response="true">
										<selection selected="false" text="Not Sure"/>
										<selection selected="false" text="No"/>
										<selection selected="false" text="Yes"/>
									</item_response>
								</question>
							</selection>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If Yes, many days per week does the infant receive lipids?">
									<free_form_response height="3"/>
								</question>
							</selection>
						</item_response>
					</question>
		
					<question mandatory="true" prompt="Who is managing the infant's TPN fluids / Lipid Solution?">
						<table_response add_row_increment="1" initial_rows_to_display="3" row_height="2">
							<column_definitions>
								<column_definition label="Physician Name" width="20"/>
								<column_definition label="Address" width="20"/>
								<column_definition label="City" width="15"/>
								<column_definition label="State" width="15"/>
								<column_definition label="Phone" width="15"/>
								<column_definition label="TPN Pharmacy Name" width="20"/>
								<column_definition label="TPN Pharmacy Number" width="15"/>
							</column_definitions>
						</table_response>
					</question>
								
				</selection>
			</item_response>
		</question>
			
		<question mandatory="true" prompt="Is the infant dependent on IV fluids?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not Sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
				
					<question mandatory="true" prompt="If yes, how long has the infant been receiving IV fluids?">
						<free_form_response height="3"/>
					</question>
					
					<question mandatory="true" prompt="If yes, how many times per day and days per week does the infant receive IV fluids?">
						<free_form_response height="3"/>
					</question>
					
					<question mandatory="true" prompt="Who is managing the infant's IV fluids?">
						<table_response add_row_increment="1" initial_rows_to_display="3" row_height="2">
							<column_definitions>
								<column_definition label="Physician Name" width="20"/>
								<column_definition label="Address" width="20"/>
								<column_definition label="City" width="15"/>
								<column_definition label="State" width="15"/>
								<column_definition label="Phone" width="15"/>
								<column_definition label="IV Pharmacy Name" width="20"/>
								<column_definition label="IV Pharmacy Number" width="15"/>
							</column_definitions>
						</table_response>
					</question>
								
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Is the infant receiving enteral / tube feedings?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not Sure"/>
