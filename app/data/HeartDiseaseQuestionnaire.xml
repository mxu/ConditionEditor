									</table_response>
								</question>
							</selection>
						</item_response>
					</question-->
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Are you on a special diet?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you are on a special diet, please provide the type of diet you are on:">
						<free_form_response height="3"/>
					</question>
					<question mandatory="true" prompt="If you are on a special diet, is this special diet prescribed by your physician?">
