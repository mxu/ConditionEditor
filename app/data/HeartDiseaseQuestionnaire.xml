								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="If you are on a special diet, has your physician recommended that you restrict your daily sodium (salt) intake?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If your physician recommended that you restrict your daily sodium intake, what is your recommended daily sodium allowance?">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="Do you follow the dietary recommendations/restrictions from your physician?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you ever smoked?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="Please provide details below:">
						<table_response add_row_increment="0" initial_rows_to_display="1">
							<column_definitions>
								<column_definition label="How Long &lt;BR&gt; (years/months)" pct="20"/>
								<column_definition label="# / Day" pct="20"/>
								<column_definition label="Stopped &lt;BR&gt; (month/year)" pct="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you drink alcohol? Include all forms of alcohol, i.e.: beer, wine, liquor.">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you drink alcohol, amount (drinks/week):">
