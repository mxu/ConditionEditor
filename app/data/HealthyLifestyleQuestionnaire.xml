	<question_group name="Healthy Lifestyle Questionnaire">
	<question mandatory="false" prompt="In order to help you meet your individual nutrition goals, we ask that you complete the following Healthy Lifestyle Questionnaire to the best of your ability.		
		This Healthy Lifestyle Nutrition Service seeks to help you improve your nutritional intake through practical everyday strategies you can implement into your life. Please provide us with between 1 and 3 nutrition lifestyle questions you have for our registered dietitian:">
		<table_response initial_rows_to_display="3" add_row_increment="0" row_height="5">
			<column_definitions>
				<column_definition label="Question" width="75"/>
			</column_definitions>
		</table_response>
	</question>
	<question mandatory="true" prompt="Who encouraged you to seek the advice of a registered dietitian?">
		<item_response drop_down_style="false" single_response="true">
			<selection selected="false" text="Myself"/>
			<selection selected="false" text="Family member"/>
			<selection selected="false" text="Friend"/>
			<selection selected="false" text="Physician"/>
			<selection selected="false" text="Nurse"/>
			<selection selected="false" text="Specialist"/>
			<selection selected="false" text="Other">
				<question mandatory="true" prompt="If other, please list:">
					<free_form_response height="1"/>
				</question>
			</selection>
		</item_response>
	</question>
	<question mandatory="true" prompt="What is the primary reason you are seeking advice from a registered dietitian? (e.g., Improve my work-day food choices)">
