			<selection selected="false" text="No"/>
			<selection selected="false" text="Yes">
				<question mandatory="true" prompt="If you take any nutritional supplements, please provide the information requested below:">
					<table_response initial_rows_to_display="3" add_row_increment="2">
						<column_definitions>
							<column_definition label="Vitamin / supplement" width="15"/>
							<column_definition label="Daily dosage consumed (milligrams, micrograms, ounces)" width="20"/>
						</column_definitions>
					</table_response>
				</question>
			</selection>
		</item_response>
	</question>
	<question mandatory="true" prompt="What is your height in feet and inches / meters and centimeters?">
		<free_form_response/>
	</question>
	<question mandatory="true" prompt="What is your weight (in pounds / kilograms)?">
		<free_form_response/>
	</question>
	<question mandatory="true" prompt="Patient's ethnic origin:">
		<item_response drop_down_style="false" single_response="true">
			<selection selected="false" text="Caucasian"/>
			<selection selected="false" text="African American"/>
			<selection selected="false" text="Hispanic"/>
			<selection selected="false" text="American Indian"/>
			<selection selected="false" text="Asian"/>
			<selection selected="false" text="Other">
				<question mandatory="true" prompt="Please identify other ethnic origin:">
					<free_form_response/>
				</question>
			</selection>
		</item_response>
	</question>
	<question mandatory="true" prompt="Of the selections below, which would best describe your eating patterns over the last six months?">
		<item_response drop_down_style="false" single_response="true">
			<selection selected="false" text="No dietary restrictions"/>
			<selection selected="false" text="Low fat / Low cholesterol"/>
			<selection selected="false" text="High protein / Low carbohydrate"/>
