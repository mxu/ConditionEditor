<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../condition.xsl"?>
<!-- edited with XML Spy v3.5 (http://www.xmlspy.com) by David Hotchkiss (IT) -->
<questionnaire>
	<question_group name="Diagnosis-specific Questions">
		<question mandatory="true" prompt="What is the principal (chief) question or concern you have regarding your current condition?">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem for which you are requesting a second opinion:">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="Have you experienced pain?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have experienced pain, where is the pain located?">
						<free_form_response/>
					</question>
					<question mandatory="true" prompt="Would you describe your pain as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild"/>
							<selection selected="false" text="Moderate"/>
							<selection selected="false" text="Severe"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="When did the pain start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you experienced pressure symptoms?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have experienced pressure symptoms, where is the pressure located?">
						<free_form_response/>
					</question>
					<question mandatory="true" prompt="Would you describe your pressure as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild"/>
							<selection selected="false" text="Moderate"/>
							<selection selected="false" text="Severe"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="When did the pressure start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you experienced bloating symptoms?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have experienced bloating symptoms, where is the bloating located?">
						<free_form_response/>
					</question>
					<question mandatory="true" prompt="Would you describe your bloating as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild"/>
							<selection selected="false" text="Moderate"/>
							<selection selected="false" text="Severe"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="When did the bloating start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you experienced nausea?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="Would you describe the nausea as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild"/>
							<selection selected="false" text="Moderate"/>
							<selection selected="false" text="Severe"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="When did the nausea start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you experienced vomiting?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="Would you describe the vomiting as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild"/>
							<selection selected="false" text="Moderate"/>
							<selection selected="false" text="Severe"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="When did the vomiting start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you experienced constipation?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="Would you describe the constipation as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild - bowel movement every other day"/>
							<selection selected="false" text="Moderate - bowel movement 2x per week"/>
							<selection selected="false" text="Severe - bowel movement less than 1x per week"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="When did the constipation start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you experienced diarrhea?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="Would you describe the diarrhea as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild - bowel movement 3 - 6x per day"/>
							<selection selected="false" text="Moderate - bowel movement 7 - 8x per day"/>
							<selection selected="false" text="Severe - bowel movement more than 8x per day"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="When did the diarrhea start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you noticed blood in your stool?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have noticed blood in your stool, please provide the following information:">
						<table_response initial_rows_to_display="1" add_row_increment="0">
							<column_definitions>
								<column_definition label="How Often"/>
								<column_definition label="What Period of Time"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="How is your appetite?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Increased"/>
				<selection selected="false" text="Decreased"/>
				<selection selected="false" text="Remained the same"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Can you eat a large meal without difficulty?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you recently had an unintentional weight gain?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you gained weight, how much (in pounds / kilograms) and in what period of time?">
						<table_response initial_rows_to_display="1" add_row_increment="0">
							<column_definitions>
								<column_definition label="Pounds / kilograms" width="15"/>
								<column_definition label="Over what period of time" width="30"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you recently had an unintentional weight loss?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you lost weight, how much (in pounds / kilograms) and in what period of time?">
						<table_response initial_rows_to_display="1" add_row_increment="0">
							<column_definitions>
								<column_definition label="Pounds / kilograms" width="15"/>
								<column_definition label="Over what period of time" width="30"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="What is your current weight (in pounds / killograms?)">
			<free_form_response/>
		</question>
		<question mandatory="true" prompt="What was your weight (in pounds / kilograms) one year ago?">
			<free_form_response/>
		</question>
		<question mandatory="true" prompt="What was your weight (in pounds / kilograms) before the diagnosis?">
			<free_form_response/>
		</question>
		<question mandatory="true" prompt="Have you had an increase in urinary frequency?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="Would you describe the problem as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild"/>
							<selection selected="false" text="Moderate"/>
							<selection selected="false" text="Severe"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="When did the urinary frequency start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you had blood in your urine?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="When did the bloody urine start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
					<question mandatory="true" prompt="If you have blood in your urine, how often does it occur, and over what period of time?">
						<table_response initial_rows_to_display="1" add_row_increment="0">
							<column_definitions>
								<column_definition label="How often" pct="50"/>
								<column_definition label="Period of time (hours,days)" pct="50"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you had trouble emptying your bladder?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="Would you describe the problem as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild"/>
							<selection selected="false" text="Moderate"/>
							<selection selected="false" text="Severe"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="When did the bladder problem start?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you had any abnormal vaginal bleeding?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="Would you describe the problem as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild"/>
							<selection selected="false" text="Moderate"/>
							<selection selected="false" text="Severe"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="How often, and over what period of time?">
						<table_response initial_rows_to_display="1" add_row_increment="0">
							<column_definitions>
								<column_definition label="How often" pct="50"/>
								<column_definition label="Period of time" pct="50"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you experienced any difficulty breathing?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="Would you describe the problem as...">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Mild"/>
							<selection selected="false" text="Moderate"/>
							<selection selected="false" text="Severe"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="How often, and over what period of time?">
						<table_response initial_rows_to_display="1" add_row_increment="0">
							<column_definitions>
								<column_definition label="How long" pct="50"/>
								<column_definition label="Period of time" pct="50"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Are you experiencing any symptoms or problems not listed above?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you are experiencing any symptoms or problems not listed above, please explain:">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have your usual activities of daily living...">
			<item_response single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="Stayed the same"/>
				<selection selected="false" text="Increased"/>
				<selection selected="false" text="Decreased">
					<question mandatory="false" prompt="If decreased, what has prevented you from participating in your daily activities? (for example, pain, fatigue, etc.)">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Please rate activity level below with regard to your general performance status.  Please choose one:">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Fully active, able to carry on all usual activities without restriction."/>
				<selection selected="false" text="Restricted in physically strenuous activity but able to carry out work of light or sedentary nature, example: light housework or office work."/>
				<selection selected="false" text="Ambulatory and capable of all self care but unable to carry out any work activities.  Up and about more than 50% of waking hours."/>
				<selection selected="false" text="Capable of only limited self care, confined to bed or chair more than 50% of waking hours."/>
				<selection selected="false" text="Completely disabled.  Cannot carry on any self care.  Totally confined to bed or chair."/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Are you?">
			<item_response single_response="true">
				<selection selected="false" text="Pre-menopausal">
					<question mandatory="true" prompt="If premenopausal, please provide the details below.">
						<table_response initial_rows_to_display="1" add_row_increment="0">
							<column_definitions>
								<column_definition label="Length of Cycle (days between periods)" pct="35"/>
								<column_definition label="Duration (days)" pct="15"/>
								<column_definition label="Flow (light, moderate, heavy)" pct="50"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
				<selection selected="false" text="Post-menopausal"/>
				<selection selected="false" text="Not sure"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Is your menstrual cycle irregular?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not applicable"/>
				<selection selected="false" text="Yes"/>
				<selection selected="false" text="No"/>
			</item_response>
		</question>
		<question mandatory="false" prompt="List any hormonal therapy.  Specify type and duration (i.e. birth control pills, progestin therapy, lupron therapy). List below:">
			<table_response initial_rows_to_display="1" add_row_increment="2">
				<column_definitions>
					<column_definition label="Type of hormone" pct="50"/>
					<column_definition label="Date started (month, year)" pct="25"/>
					<column_definition label="Duration (months, years)" pct="25"/>
				</column_definitions>
			</table_response>
		</question>
		<question mandatory="false" prompt="If menopausal, when did it begin?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Do you have a history of abnormal pap smears?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have a history of abnormal pap smears, was treatment required?">
						<item_response single_response="true">
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If treatment was required, what type of treatment?">
									<free_form_response/>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have any other gynecologic problems or issues?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have any other gynecologic problems or issues, please describe:">
						<free_form_response height="5"/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have a history of any other cancers or pre-cancerous conditions?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have a history of any other cancers or pre-cancerous conditions, please provide the following information:">
						<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Condition" width="20"/>
								<column_definition label="Month/Year" width="18"/>
								<column_definition label="Method of treatment" width="20"/>
								<column_definition label="Physician" width="20"/>
								<column_definition label="Hospital" width="20"/>
								<column_definition label="City/State" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have a family history of cancer?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have a family history of cancer, please provide the following information regarding your family history. Please indicate if the family member is related on your maternal or paternal side, if applicable.">
						<table_response initial_rows_to_display="3" add_row_increment="3" row_height="2">
							<column_definitions>
								<column_definition label="Family member &lt;BR&gt; (grandfather, sister, etc.)" width="25"/>
								<column_definition label="Maternal/Paternal" width="20"/>
								<column_definition label="Type of cancer" width="25"/>
								<column_definition label="Age at diagnosis &lt;BR&gt; (if known)" width="30"/>
								<column_definition label="Current condition &lt;BR&gt; of relative" width="30"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="How was your current diagnosis (one you are seeking second opinion for) made? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="X-Rays"/>
				<selection selected="false" text="CT"/>
				<selection selected="false" text="IVP"/>
				<selection selected="false" text="KUB"/>
				<selection selected="false" text="Ultrasound"/>
				<selection selected="false" text="Surgery"/>
				<selection selected="false" text="Biopsy"/>
				<selection selected="false" text="Pap Smear"/>
				<selection selected="false" text="Blood Work"/>
				<selection selected="false" text="Unknown"/>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If other, please list:">
						<table_response initial_rows_to_display="2" add_row_increment="2" row_height="1">
							<column_definitions>
								<column_definition label="Type of study" pct="100"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="When was your diagnosis made?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="When were you last examined?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		<question mandatory="true" prompt="Have you had any of the following studies?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have had any of the following studies, please indicate below: (complete all that apply)">
						<table_response initial_rows_to_display="3" add_row_increment="2">
							<column_definitions>
								<column_definition label="Study" pct="70"/>
								<column_definition label="Month" pct="15"/>
								<column_definition label="Year" pct="15"/>
							</column_definitions>
							<table_data>
								<cell>CT</cell>
								<cell/>
								<cell/>
							</table_data>
							<table_data>
								<cell>MRI</cell>
								<cell/>
								<cell/>
							</table_data>
							<table_data>
								<cell>Other x-rays</cell>
								<cell/>
								<cell/>
							</table_data>
							<table_data>
								<cell>Chest X-ray</cell>
								<cell/>
								<cell/>
							</table_data>
							<table_data>
								<cell>IVP</cell>
								<cell/>
								<cell/>
							</table_data>
							<table_data>
								<cell>KUB</cell>
								<cell/>
								<cell/>
							</table_data>
							<table_data>
								<cell>Ultrasound</cell>
								<cell/>
								<cell/>
							</table_data>
							<table_data>
								<cell>Other</cell>
								<cell/>
								<cell/>
							</table_data>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>			
		<question mandatory="true" prompt="Have you undergone surgery / surgical procedure(s) for your current condition (one you are seeking opinion for)?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have undergone surgery / surgical procedure(s) for your current condition, please complete the following information:">
						<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Surgery/Procedure" width="25"/>
								<column_definition label="Month/Year" width="20"/>
								<column_definition label="Physician" width="20"/>
								<column_definition label="Hospital/Clinic" width="20"/>
								<column_definition label="City, State" width="20"/>
							</column_definitions>
						</table_response>
					</question>
					<question mandatory="true" prompt="If you have undergone surgical treatment, what is your general subjective impression of the experience?  (In your own words)">
						<free_form_response height="5"/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you undergone chemotherapy for your current condition (one you are seeking opinion for)?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have undergone chemotherapy for your current condition, please complete the following information:">
