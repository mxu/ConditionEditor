<questionnaire>
	<question_group name="Diagnosis-specific Questions">
		<question mandatory="true" prompt="What is the principal (chief) question or concern you have regarding your current condition?">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem for which you are requesting a second opinion:">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="Does the patient have a family history of any of the following conditions? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Parkinson's disease"/>
				<selection selected="false" text="Alzheimer's disease"/>
				<selection selected="false" text="Gait or walking problems"/>
				<selection selected="false" text="Dementia"/>
				<selection selected="false" text="None of the above"/>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If other, please explain:">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Has the patient been diagnosed with any of the following conditions?  (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="NPH (Normal pressure hydrocephalus)"/>
				<selection selected="false" text="Alzheimer's disease"/>
				<selection selected="false" text="Arthritis"/>
				<selection selected="false" text="Atypical Parkinson's disease"/>
				<selection selected="false" text="Back surgery"/>
				<selection selected="false" text="Bipolar disorder"/>
				<selection selected="false" text="Cervical myelopathy"/>
				<selection selected="false" text="Compression fractures"/>
				<selection selected="false" text="Dementia"/>
				<selection selected="false" text="Depression"/>
				<selection selected="false" text="Diabetes"/>
				<selection selected="false" text="Obsessive compulsive disorder"/>
				<selection selected="false" text="Parkinson's disease"/>
				<selection selected="false" text="Peripheral neuropathy"/>
				<selection selected="false" text="Schizophrenia"/>
				<selection selected="false" text="Stroke/TIA's/Cerebral vascular disease"/>
				<selection selected="false" text="Spinal disease"/>
				<selection selected="false" text="Seizures"/>
				<selection selected="false" text="None of the above"/>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If other, please explain:">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="What is the patient's diagnosis?">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="What is the specialty of the physician or physicians who made the diagnosis? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Neurosurgeon"/>
				<selection selected="false" text="Neurologist"/>
				<selection selected="false" text="General practitioner"/>
				<selection selected="false" text="Primary care provider"/>
				<selection selected="false" text="Psychiatrist"/>
				<selection selected="false" text="Internal medicine"/>
				<selection selected="false" text="Geriatrics"/>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If other, please explain:">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient have difficulty with balance or with lifting their feet when walking? (Does the patient feel like their feet are sticking to the floor?)">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient has difficulty with balance or with lifting their feet when walking, when did this symptom begin?">
									&MONTH_YEAR_TABLE_RESPONSE;
					</question>
					<question mandatory="true" prompt="For how long has this symptom been present?:">
						<free_form_response height="2"/>
					</question>
					<question mandatory="true" prompt="Was there an event (for example trauma or surgery) that resulted in the onset of this symptom?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If there was an event that resulted in the onset of this symptom, please explain:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient experience difficulty in walking?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient experiences difficulty in walking, which of the following statements best describes the problem with walking? (choose one)">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="Walking is slowed, unsteady and/or wide based but secure without assistance"/>
							<selection selected="false" text="Walking is difficult and only possible with assistance"/>
							<selection selected="false" text="Can walk only walk a few steps with assistance of others"/>
							<selection selected="false" text="Impossible to walk"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If other, please describe:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Is the patient's walking affected by? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Tremor"/>
				<selection selected="false" text="Stiffness"/>
				<selection selected="false" text="Slowness"/>
				<selection selected="false" text="Movement freezing"/>
				<selection selected="false" text="Involuntary or uncontrolled movements"/>
				<selection selected="false" text="Muscle spasms"/>
				<selection selected="false" text="None of the above"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient experience pain associated with walking?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient experiences pain associated with walking, please indicate the location of the pain below: (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Localized hip pain"/>
							<selection selected="false" text="Right hip"/>
							<selection selected="false" text="Left hip"/>
							<selection selected="false" text="Both hips"/>
							<selection selected="false" text="Localized knee pain"/>
							<selection selected="false" text="Right knee"/>
							<selection selected="false" text="Left knee"/>
							<selection selected="false" text="Both knees"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If other, please describe:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient experience neck pain, neck numbness, arm pain or arm numbness?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient experiences neck pain, neck numbness, arm pain or arm numbness, please indicate the location of the pain or numbness below: (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Neck pain"/>
							<selection selected="false" text="Neck numbness"/>
							<selection selected="false" text="Right arm pain"/>
							<selection selected="false" text="Right arm numbness"/>
							<selection selected="false" text="Left arm pain"/>
							<selection selected="false" text="Left arm numbness"/>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient experience leg pain, leg numbness or leg weakness?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient experiences leg pain, leg numbness or leg weakness, please indicate the location of the pain, numbness or weakness below: (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Right leg pain"/>
							<selection selected="false" text="Right leg numbness"/>
							<selection selected="false" text="Right leg weakness"/>
							<selection selected="false" text="Left leg pain"/>
							<selection selected="false" text="Left leg numbness"/>
							<selection selected="false" text="Left leg weakness"/>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient experience back pain?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient experiences back pain, what is the location of the back pain?:">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient use any of the following assistive devices? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Cane"/>
				<selection selected="false" text="Walker"/>
				<selection selected="false" text="Scooter"/>
				<selection selected="false" text="Wheelchair"/>
				<selection selected="false" text="None of the above"/>
				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If other, please explain:">
						<free_form_response/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient fall?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient falls, how many times per month does the patient fall?">
						<free_form_response height="1"/>
					</question>
					<question mandatory="true" prompt="Describe the circumstances that surround or cause the patient's falling:">
						<free_form_response height="5"/>
					</question>
					<question mandatory="true" prompt="Has the patient sustained any significant fall related injuries?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If the patient has sustained any significant fall related injuries, please explain:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Has the patient undergone any type of surgical treatment to improve his / her ability to walk? (For example, back surgery or joint replacement.)">
			<item_response single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient has undergone any type of surgical treatment to improve his / her ability to walk, please complete the following information:">
						<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Type of Surgery" width="25"/>
								<column_definition label="Date of Surgery" width="20"/>
								<column_definition label="Name of Physician" width="20"/>
								<column_definition label="Name of Hospital" width="20"/>
								<column_definition label="Location of Hospital (City, State)" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient experience lightheadedness and / or dizziness?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient experiences lightheadedness and / or dizziness, when did this symptom begin?">
									&MONTH_YEAR_TABLE_RESPONSE;
					</question>
					<question mandatory="true" prompt="Which of the following statements best describes the problem with light headedness or dizziness? (choose one)">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="Intermittent spells"/>
							<selection selected="false" text="Permanent or continuous"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is this symptom associated with fainting and / or loss of consciousness?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If this symptom is associated with fainting and / or loss of consciousness, please explain:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is this symptom associated with hearing loss?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Is this symptom associated with tinnitus or ringing in the ears?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient have a history of any of the following? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Diagnosis of vertigo"/>
				<selection selected="false" text="Cerebral vascular disease"/>
				<selection selected="false" text="Stroke"/>
				<selection selected="false" text="TIA's (transient ischemic attacks)"/>
				<selection selected="false" text="Cardiac arrhythmias"/>
				<selection selected="false" text="Head trauma"/>
				<selection selected="false" text="Meniere's disease"/>
				<selection selected="false" text="None of the above"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient experience forgetfulness or confusion?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient experiences forgetfulness or confusion, when did this symptom begin?">
									&MONTH_YEAR_TABLE_RESPONSE;
					</question>
					<question mandatory="true" prompt="Which of the following statements BEST describes the problem with forgetfulness or confusion? (choose one)">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="Problem with concentration and/or forgetfulness"/>
							<selection selected="false" text="Problem with concentration and forgetfulness with apathy and some disorientation"/>
							<selection selected="false" text="Complete disorientation and skill disorder"/>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient experience any of the following symptoms? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Anxiety"/>
				<selection selected="false" text="Depression"/>
				<selection selected="false" text="Sleep problems"/>
				<selection selected="false" text="Obstructive sleep apnea"/>
				<selection selected="false" text="Hallucinations"/>
				<selection selected="false" text="Paranoia"/>
				<selection selected="false" text="Mania"/>
				<selection selected="false" text="None of the above"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient have a history of any of the following? (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Brain tumor"/>
				<selection selected="false" text="Cerebral infection"/>
				<selection selected="false" text="Congenital anomaly"/>
				<selection selected="false" text="ECT (electroconvulsive therapy)"/>
				<selection selected="false" text="Head trauma"/>
				<selection selected="false" text="ICH (Intracerebral hematoma)"/>
				<selection selected="false" text="SAH (Subarachnoid hemorrhage)"/>
				<selection selected="false" text="None of the above"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Has the patient experienced a loss of bladder function?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient has experienced a loss of bladder function, when did this symptom begin?">
									&MONTH_YEAR_TABLE_RESPONSE;
					</question>
					<question mandatory="true" prompt="Was there an event (for example, trauma or surgery) that resulted in the onset of this symptom?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If there was a sentinel event that resulted in the onset of this symptom, please explain:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="What best describes this bladder symptom?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="Urgency of urination"/>
							<selection selected="false" text="Episodes of incontinence"/>
							<selection selected="false" text="Permanent incontinence"/>
							<selection selected="false" text="Incontinence of both urine and stool"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If other, please explain:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="Does the patient require the use of Depends or other protective undergarments?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Does the patient wake during the night to use the toilet?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If the patient wakes during the night to use the toilet, on average how many trips to the toilet are made each night?">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient have any of the following?">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Prolapsed bladder"/>
				<selection selected="false" text="Prostate disease"/>
				<selection selected="false" text="None of the above"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Has the patient undergone any surgical treatment to improve control of urination?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient has undergone any surgical treatment to improve control of urination, please provide the following information:">
						<table_response initial_rows_to_display="3" add_row_increment="2" row_height="2">
							<column_definitions>
								<column_definition label="Type of Surgery" width="30"/>
								<column_definition label="Date of Surgery" width="15"/>
								<column_definition label="Outcome/Benefit" width="30"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Does the patient experience headaches?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If the patient experiences headaches, when did this symptom begin?">
									&MONTH_YEAR_TABLE_RESPONSE;
					</question>
					<question mandatory="true" prompt="Was there an event (for example, trauma or surgery) that resulted in the onset of this symptom?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If there was a sentinel event that resulted in the onset of this symptom, please describe the event below:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="On what days of the week does this symptom occur?">
						<free_form_response height="2"/>
					</question>
					<question mandatory="true" prompt="What is the location (one or both sides of the head, front or back of the head, neck) of the pain?">
						<free_form_response height="2"/>
					</question>
					<question mandatory="true" prompt="What is the quality (pressure, pounding, sharp, band-like, stabbing, etc.) of the pain?">
						<free_form_response height="2"/>
					</question>
					<question mandatory="true" prompt="Is the headache a slight headache which occurs intermittently or a constant heavy headache?  (Please describe in detail below.)">
						<free_form_response height="5"/>
					</question>
					<question mandatory="true" prompt="Does the pain stay in one location or move or progress from one area of the head to another? (Please describe in detail below.)">
						<free_form_response height="5"/>
					</question>
					<question mandatory="true" prompt="Does anything trigger the headache?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes">
								<question mandatory="true" prompt="If anything triggers the headache, please describe in detail:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
					<question mandatory="true" prompt="Does the patient awaken with a headache?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Does the patient ever awaken from a sound sleep with a headache?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="No"/>
							<selection selected="false" text="Yes"/>
						</item_response>
					</question>
					<question mandatory="true" prompt="Which of the following symptoms precede or accompany the patient's headache? (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Light sensitivity"/>
							<selection selected="false" text="Blind spots"/>
							<selection selected="false" text="Runny or stuffy nose"/>
							<selection selected="false" text="Itchy eyes"/>
							<selection selected="false" text="Flashes of light"/>
							<selection selected="false" text="Blurred vision"/>
							<selection selected="false" text="Double vision"/>
							<selection selected="false" text="Noise sensitivity"/>
							<selection selected="false" text="Dizziness or unsteadiness"/>
							<selection selected="false" text="Numbness or tingling"/>
							<selection selected="false" text="Nausea"/>
