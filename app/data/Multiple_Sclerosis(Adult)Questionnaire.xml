<questionnaire>
	<question_group name="Diagnosis-specific Questions">
	
		<question mandatory="true" prompt="What is the principal (chief) question or concern you have regarding patient's current condition?">
			<free_form_response height="5"/>
		</question>
				
		<question mandatory="true" prompt="What is the patient hoping to gain from this second opinion consultation?">
			<free_form_response height="5"/>
		</question>
		
		<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem for which the patient is seeking a second opinion? &lt;BR&gt;(Please focus more on what symptoms the patient is experiencing and less on what the doctors have said)">
			<free_form_response height="5"/>
		</question>
		
		<question mandatory="true" prompt="What is the patient's current diagnosis? (please provide exact diagnosis if known)">
			<free_form_response height="5"/>
		</question>
		
		<question mandatory="true" prompt="When did the patient's symptoms begin?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		
		<question mandatory="true" prompt="Describe the patient's initial symptoms below (be as specific as possible in your description):">
			<free_form_response height="5"/>
		</question>
		
		<question mandatory="true" prompt="Please list the patient's summary of events related to this diagnosis below. Please record the events in date order: &lt;BR&gt;(Episodes of symptoms should be listed in date order with a description of each symptom)">
			<table_response initial_rows_to_display="4" add_row_increment="3" row_height="2">
				<column_definitions>
					<column_definition label="Episodes of symptoms" width="30"/>
					<column_definition label="Description" width="35"/>
					<column_definition label="Date (mm/dd/yy)" width="25"/>
				</column_definitions>
			</table_response>
		</question>
		
		<question mandatory="true" prompt="When was the patient's diagnosis made?">
			&MONTH_YEAR_TABLE_RESPONSE;
		</question>
		
		<question mandatory="true" prompt="Did Patient ever recieved any treatments (procedure) for the condition?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, Please list all treatments that have been tried and the effect of the treatment on the patient">
						<table_response add_row_increment="1" initial_rows_to_display="3" row_height="2">
							<column_definitions>
								<column_definition label="Treatment Name" width="35"/>
								<column_definition label="Date(s) of the treatment (mm/dd/yyyy)" width="15"/>
								<column_definition label="Effect on patient " width="25"/>
								<column_definition label="Comments" width="25"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="Please complete the below table with patient's experience of the symptoms">
			<table_response add_row_increment="2" row_height="2">
				<column_definitions>
					<column_definition label="Symptoms" width="35"/>
					<column_definition label="Description" width="40"/>
				</column_definitions>
				<table_data>
					<cell>Loss of vision in one or both eyes lasting days or weeks</cell>
					<cell/>
				</table_data>
				<table_data>
					<cell>Numbness or weakness of arms lasting days or weeks</cell>
					<cell/>
				</table_data>
				<table_data>
					<cell>Numbness or weakness of legs lasting days or weeks</cell>
					<cell/>
				</table_data>
				<table_data>
					<cell>Numbness or weakness of body lasting days or weeks</cell>
					<cell/>
				</table_data>
				<table_data>
					<cell>Walking problems</cell>
				</table_data>
				<table_data>
					<cell>Difficulty with urination</cell>
					<cell/>
				</table_data>
				<table_data>
					<cell>Difficulty tolerating heat</cell>
					<cell/>
				</table_data>
				<table_data>
					<cell>Visual blurring with exercise</cell>
					<cell/>
				</table_data>
				<table_data>
					<cell>Dizziness</cell>
					<cell/>
				</table_data>
				<table_data>
					<cell>Memory difficulty</cell>
					<cell/>
				</table_data>
				<table_data>
					<cell>Fatigue</cell>
					<cell/>
				</table_data>
			</table_response>
		</question>

		<question mandatory="true" prompt="Has the Patient experienced any of the following symptoms: (choose all that apply)">
			<item_response drop_down_style="false" single_response="false">
				<selection selected="false" text="Sleep Problems">
					<question mandatory="true" prompt=" If yes, please describe below:">
						<free_form_response height="5"/>
					</question>
				</selection>
				<selection selected="false" text="Depression">
					<question mandatory="true" prompt=" If yes, please describe below:">
						<free_form_response height="5"/>
					</question>
				</selection>
				<selection selected="false" text="Pain">
					<question mandatory="true" prompt="If yes to pain, what is the location of the pain?">
						<free_form_response height="5"/>
					</question>
					
					<question mandatory="true" prompt="Please Describe pain?">
						<free_form_response height="5"/>
					</question>
					
					<question mandatory="true" prompt="Please rate your pain on the scale below: (1= Minimal pain, 10= Worst pain)">
						<item_response drop_down_style="true" single_response="true">
							<selection selected="false" text="10 - Worst pain"/>
							<selection selected="false" text="9"/>
							<selection selected="false" text="8"/>
							<selection selected="false" text="7"/>
							<selection selected="false" text="6"/>
							<selection selected="false" text="5 - Moderate pain"/>
							<selection selected="false" text="4"/>
							<selection selected="false" text="3"/>
							<selection selected="false" text="2"/>
							<selection selected="false" text="1 - Minimal pain"/>
						</item_response>
					</question>
					
					<question mandatory="true" prompt=" How long have you had the pain?">
						<free_form_response height="5"/>
					</question>
					
					<question mandatory="true" prompt="Is the pain constant or intermittent?">
						<item_response drop_down_style="false" single_response="true">
							<selection selected="false" text="Constant pain"/>
							<selection selected="false" text="Intermittent pain"/>
						</item_response>
					</question>
					
					<question mandatory="true" prompt="What makes the pain worse?">
						<free_form_response height="5"/>
					</question>
										
					<question mandatory="true" prompt="What makes the pain better?">
						<free_form_response height="5"/>
					</question>
					
					<question mandatory="true" prompt="Please list all pain treatments that have been tried and the effect of the treatment on the patient">
						<table_response add_row_increment="1" initial_rows_to_display="3" row_height="2">
							<column_definitions>
								<column_definition label="Treatment Name" width="35"/>
								<column_definition label="Effect" width="25"/>
								<column_definition label="Comments" width="25"/>
							</column_definitions>
						</table_response>
					</question>
					
				</selection>
				<selection selected="false" text="Does not have any of these symptoms"/>
			</item_response>
		</question>
		
		<question mandatory="true" prompt="In the table below, please list all medications the patient has taken for the diagnosis which he/she is seeking an opinion, the dosage, date started and stopped, positive effects, and side effects">
			<table_response add_row_increment="1" initial_rows_to_display="3" row_height="2">
				<column_definitions>
					<column_definition label="Medication" width="25"/>
					<column_definition label="Dosage" width="15"/>
					<column_definition label="Date Started&lt;BR&gt;(Month/Year)" width="10"/>
					<column_definition label="Date Stopped&lt;BR&gt;(Month/Year)" width="10"/>
					<column_definition label="Reason Stopped" width="20"/>
					<column_definition label="Positive Effects" width="20"/>
					<column_definition label="Side Effects" width="20"/>
					<column_definition label="Comments" width="20"/>
				</column_definitions>
			</table_response>
		</question>
		
		<question mandatory="true" prompt="In the table below, please list all treatments / therapies the patient has tried for the diagnosis which he/she is seeking an opinion, the date started and stopped, positive effects, and side effects">
			<table_response add_row_increment="1" initial_rows_to_display="3" row_height="2">
				<column_definitions>
					<column_definition label="Treatment / Therapy" width="25"/>
					<column_definition label="Date Started&lt;BR&gt;(Month/Year)" width="10"/>
					<column_definition label="Date Stopped&lt;BR&gt;(Month/Year)" width="10"/>
					<column_definition label="Reason Stopped" width="20"/>
					<column_definition label="Positive Effects" width="20"/>
					<column_definition label="Side Effects" width="20"/>
					<column_definition label="Comments" width="20"/>
				</column_definitions>
			</table_response>
		</question>
		
		<question mandatory="true" prompt="Is there a family history of Multiple Sclerosis in a blood relative (parents, sisters, brothers, grandparents, aunts, uncles, etc.)?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
