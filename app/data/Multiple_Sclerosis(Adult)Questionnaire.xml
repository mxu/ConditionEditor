				<selection selected="false" text="Colon Cancer"/>
				<selection selected="false" text="Breast Cancer"/>
				<selection selected="false" text="Other Cancer">
					<question mandatory="true" prompt="If other, describe list below:">
						<free_form_response height="4"/>
					</question>
				</selection>
				<selection selected="false" text="Colon Polyps"/>
				<selection selected="false" text="Thyroid Disease"/>
				<selection selected="false" text="Kidney Stones"/>
				<selection selected="false" text="Kidney Failure"/>
				<selection selected="false" text="Kidney Disease"/>
				<selection selected="false" text="Nervous Breakdown"/>
