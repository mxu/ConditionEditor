						<selection selected="false" text="Yes"/>
						<selection selected="false" text="No"/>
					</item_response>
					</question>
				</selection>
				<selection selected="false" text="My stomach is easily upset ">
				 <question mandatory="true" prompt="">
					<item_response drop_down_style="false" single_response="true">
						<selection selected="false" text="Yes"/>
						<selection selected="false" text="No"/>
					</item_response>
					</question>
				</selection>
				<selection selected="false" text="I get diarrhea often ">
				 <question mandatory="true" prompt="">
					<item_response drop_down_style="false" single_response="true">
						<selection selected="false" text="Yes"/>
						<selection selected="false" text="No"/>
					</item_response>
					</question>
				</selection>
				<selection selected="false" text="I will be living in a dormitory ">
				 <question mandatory="true" prompt="">
					<item_response drop_down_style="false" single_response="true">
						<selection selected="false" text="Yes"/>
						<selection selected="false" text="No"/>
					</item_response>
					</question>
