				<selection selected="false" text="Vacation / pleasure"/>
				<selection selected="false" text="Business"/>
				<selection selected="false" text="Adoption"/>
				<selection selected="false" text="To provide medical care"/>
				<selection selected="false" text="To receive medical care"/>
				<selection selected="false" text="Research"/>
				<selection selected="false" text="Study abroad "/>
				<selection selected="false" text="Hunting"/>
				<selection selected="false" text="Emergency, last-minute notice"/>
				<selection selected="false" text="Travel to region of personal or family origin to visit relatives and/or friends"/>
