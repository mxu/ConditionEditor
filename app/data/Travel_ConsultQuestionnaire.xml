<?xml-stylesheet type="text/xsl" href="../condition.xsl"?>

<questionnaire>
<question_group name="Travel Information">
	<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem for which you are requesting a second opinion:">
			<free_form_response height="5"/>
	</question>
	<question mandatory="false" prompt="Please list your specific questions related to your travel plans:">
			<table_response initial_rows_to_display="3" add_row_increment="0" row_height="5">
				<column_definitions>
					<column_definition label="Question" width="75"/>
				</column_definitions>
			</table_response>
	</question>
	<question mandatory="true" prompt="What is the country of your birth ?">
			<free_form_response height="1"/>
	</question>
		<question mandatory="true" prompt="Will you be traveling with any family members ?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, please list name and relationship:">
						<table_response initial_rows_to_display="3" add_row_increment="3" row_height="2">
							<column_definitions>
								<column_definition label="Name " width="50"/>
								<column_definition label="Relationship" width="50"/>					
								<column_definition label="Age" width="20"/>
							</column_definitions>	
						</table_response>
					</question>
				</selection>
			</item_response>			
		</question>
		
		<question mandatory="true" prompt="Date of Scheduled Departure">
			<free_form_response height="1"/>
		</question>
		<question mandatory="true" prompt="Scheduled or Estimated Date of Return">
			<free_form_response height="1"/>
		</question>
		
		<question mandatory="true" prompt="Please list country(ies) and city(ies) you will be traveling to in order of travel plans:">
