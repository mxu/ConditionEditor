						<selection selected="false" text="Yes"/>
						<selection selected="false" text="No"/>
					</item_response>
					</question>
				</selection>
				<selection selected="false" text="Taking pills to prevent malaria seems unnecessary ">
				 <question mandatory="true" prompt="">
