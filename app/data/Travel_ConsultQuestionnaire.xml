				<column_definitions>
					<column_definition label="Country" width="35"/>
					<column_definition label="City" width="25"/>
					<column_definition label="Length of stay (number of days / weeks / months)" width="50"/>
					<column_definition label="Purpose of Travel" width="35"/>
				</column_definitions>
			</table_response>			
		</question>
		
		<question mandatory="true" prompt="While living overseas, do you plan to take side trips to places other than those listed above?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If yes, please complete information below: ">
						<table_response initial_rows_to_display="3" add_row_increment="3" row_height="2">
							<column_definitions>
								<column_definition label="Country" width="35"/>
								<column_definition label="City" width="25"/>
								<column_definition label="Length of stay (number of days / weeks / months)" width="35"/>							
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>			
		</question>
		
		<question mandatory="true" prompt="Will you travel to altitudes over 5000 feet (1500 meters) ?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
