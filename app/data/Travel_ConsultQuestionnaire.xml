					<question mandatory="true" prompt="please enter details: ">
						<free_form_response height="3"/>
					</question>	
				</selection>
				<selection selected="false" text="Other activities">
					<question mandatory="false" prompt="please enter details: ">
						<free_form_response height="3"/>
					</question>	
				</selection>				
			</item_response>										
		</question>
		
		<question mandatory="true" prompt="Please answer the questions below related to you and your travel plans">
