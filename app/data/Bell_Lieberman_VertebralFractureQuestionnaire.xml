<?xml-stylesheet type="text/xsl" href="../condition.xsl"?>
<questionnaire>
	<question_group name="Diagnosis-specific Questions">
		<question mandatory="true" prompt="What is the principal (chief) question or concern you have regarding your current condition?">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="In your own words, please provide a brief narrative of the details of the problem for which you are requesting a second opinion:">
			<free_form_response height="5"/>
		</question>
		<question mandatory="true" prompt="For how long have you had this problem?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="less than 1 Month"/>
				<selection selected="false" text="1 - 3 Months"/>
				<selection selected="false" text="4 - 6 Months"/>
				<selection selected="false" text="7 - 12 Months"/>
				<selection selected="false" text="greater than 12 Months"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Which areas of your body have the worse pain?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Neck"/>
				<selection selected="false" text="Back"/>
				<selection selected="false" text="Arm"/>
				<selection selected="false" text="Leg"/>
				<selection selected="false" text="Neck and arm pain are equal"/>
				<selection selected="false" text="Back and leg pain are equal"/>

				<selection selected="false" text="Other">
					<question mandatory="true" prompt="If other, please describe:">
						<free_form_response height="1"/>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Is the problem?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Getting worse"/>
				<selection selected="false" text="Getting better"/>
				<selection selected="false" text="Staying the same"/>
				<selection selected="false" text="Not sure"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you had neck or back problems?">
			<item_response single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have had neck or back problems, when?">
						&MONTH_YEAR_TABLE_RESPONSE;
					</question>
					<question mandatory="true" prompt="What was the cause of your previous neck / back trouble?">
						<item_response single_response="true">
							<selection selected="false" text="Not sure"/>
							<selection selected="false" text="Work injury"/>
							<selection selected="false" text="Road traffic accident"/>
							<selection selected="false" text="Sports injury"/>
							<selection selected="false" text="No specific injury"/>

							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If other, please describe:">
									<free_form_response height="2"/>
								</question>
							</selection>
						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you think the fault of your current neck/back condition is?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="Nobody's"/>
				<selection selected="false" text="Yours"/>
				<selection selected="false" text="Your employer's"/>
				<selection selected="false" text="A co-worker's"/>
				<selection selected="false" text="Another person's"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you now or have you ever had a Worker's Compensation claim involving this problem?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes, now"/>
				<selection selected="false" text="Yes, previously"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have litigation (lawsuit) pending involving this problem?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Do you have a family history of back and / or neck problems?">
			<item_response drop_down_style="false" single_response="true">
				<selection selected="false" text="Not sure"/>
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes"/>
			</item_response>
		</question>
		<question mandatory="true" prompt="Have you had previous neck or back surgery?">
			<item_response single_response="true">
				<selection selected="false" text="No"/>
				<selection selected="false" text="Yes">
					<question mandatory="true" prompt="If you have had previous neck or back surgery, please provide the following information:">
						<table_response initial_rows_to_display="3" add_row_increment="3" row_height="1">
							<column_definitions>
								<column_definition label="Type of surgery/procedure" width="25"/>
								<column_definition label="Month/Year" width="20"/>
								<column_definition label="Physician" width="20"/>
								<column_definition label="Hospital/Clinic" width="20"/>
								<column_definition label="City, State" width="20"/>
							</column_definitions>
						</table_response>
					</question>
				</selection>
			</item_response>
		</question>
		<question prompt="Are you experiencing pain?" mandatory="true">
			<item_response drop_down_style="false" single_response="true">
				<selection text="No" selected="false"/>
				<selection text="Yes" selected="false">
					<question mandatory="true" prompt="If you are experiencing pain, where is the pain located?">
						<free_form_response height="2"/>
					</question>
					<question prompt="Please rate your pain below: (1= minimal pain, 10= worst pain)" mandatory="true">
						<item_response drop_down_style="true" single_response="true">
							<selection selected="false" text="10 - Worst pain"/>
							<selection selected="false" text="9"/>
							<selection selected="false" text="8"/>
							<selection selected="false" text="7"/>
							<selection selected="false" text="6"/>
							<selection selected="false" text="5 - Moderate Pain"/>
							<selection selected="false" text="4"/>
							<selection selected="false" text="3"/>
							<selection selected="false" text="2"/>
							<selection selected="false" text="1 - Minimal pain"/>
						</item_response>

					</question>
					<question mandatory="true" prompt="Indicate which of the following activities increase your pain: (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Exercise"/>
							<selection selected="false" text="Sitting"/>
							<selection selected="false" text="Standing"/>
							<selection selected="false" text="Walking"/>
							<selection selected="false" text="Twisting"/>
							<selection selected="false" text="Bending forward"/>
							<selection selected="false" text="Bending backward"/>
							<selection selected="false" text="Coughing"/>
							<selection selected="false" text="Sneezing"/>
							<selection selected="false" text="Sleeping"/>
							<selection selected="false" text="Walking up stairs"/>
							<selection selected="false" text="Walking down stairs"/>
							<selection selected="false" text="Other">
								<question mandatory="true" prompt="If other, please list:">
									<free_form_response/>
								</question>
							</selection>

						</item_response>
					</question>
					<question mandatory="true" prompt="Indicate which of the following activities decrease your pain: (choose all that apply)">
						<item_response drop_down_style="false" single_response="false">
							<selection selected="false" text="Lying down"/>
							<selection selected="false" text="Sitting"/>
							<selection selected="false" text="Standing"/>
							<selection selected="false" text="Walking"/>
							<selection selected="false" text="Physical therapy"/>
							<selection selected="false" text="Manipulator"/>
							<selection selected="false" text="Pain medications"/>
							<selection selected="false" text="Muscle relaxant medications"/>
							<selection selected="false" text="Anti-inflammatory medications"/>
							<selection selected="false" text="Other activities">
								<question mandatory="true" prompt="If other activities, please list:">
									<free_form_response/>
								</question>
							</selection>
							<selection selected="false" text="Other medications">
								<question mandatory="true" prompt="If other medications, please list:">
									<free_form_response/>
								</question>
							</selection>

						</item_response>
					</question>
				</selection>
			</item_response>
		</question>
